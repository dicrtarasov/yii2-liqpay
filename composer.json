{
    "name": "dicr/yii2-liqpay",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Библиотека платежей LiqPay для Yii2",
    "keywords": ["dicr", "php", "yii2", "liqpay"],
    "homepage": "https://gitlab.com/dicr/yii2-liqpay",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-liqpay.git"
    },
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "dicr\\liqpay\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
